{
    "packages": [
      "python@3.10",
      "stdenv.cc.cc.lib",
      "amass@3.22.2",
      "subfinder@2.6.0",
      "httpx@1.3.3",
      "ipinfo@2.10.1",
      "naabu@2.1.6",
      "nuclei@2.9.7",
      "gitleaks@8.17.0",
      "gau@2.1.2",
      "python310Packages.pip"
    ],
    "shell": {
      "init_hook": [
        ". $VENV_DIR/bin/activate",
        "export PS1='ðŸ¦— Mantis > '",
        "alias mantis='python /usr/local/mantis/launch.py'",
        "alias help='python /usr/local/mantis/launch.py --help'",
        "cd /usr/local/mantis"
      ],
      "scripts": {
        "setup": [
          ". $VENV_DIR/bin/activate",
          "echo $(which pip)",
          "pip install git+https://github.com/cisagov/findcdn.git --quiet",
          "pip install $(grep -ivE 'ray' -r requirements.txt)"
        ]
      }
    }
  }
  